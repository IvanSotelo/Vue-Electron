{
  "name": "{{ name }}",
{{#if_eq builder 'packager'}}
  "productName": "{{ name }}",
{{/if_eq}}
  "version": "0.0.0",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    {{/if_eq}}
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    {{#if e2e}}
    {{#if_eq e2eConfig 'nightwatch'}}
    "e2e": "node test/e2e/runner.js",
    {{else}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if_eq}}
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    {{#if_eq unitConfig 'mocha'}}
    "unit": "karma start test/unit/karma.conf.js",
    {{else}}
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    {{/if_eq}}
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
  "build": {
    "productName": "{{ name }}",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
{{/if_eq}}
  "dependencies": {
    "epic-spinners": "^1.0.3",
    "gsap": "^1.20.4",
    "hamsterjs": "^1.1.3",
    "howler": "^2.0.9",
    "image-preloader": "^1.1.1",
    "isotope-layout": "^3.0.6",
    "masonry-layout": "^4.2.1",
    "pug": "^2.0.3",
    "smooth-scrolling": "^2.3.9",
    "sniffer": "github:watsondg/sniffer",
    "swiper": "^4.2.2",
    "vue": "^3.0.0"{{deps plugins}}
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "chromedriver": "^2.27.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.0",
    "electron-debug": "^2.1.2",
    "electron-devtools-installer": "^2.2.4",

    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^1.3.3",
    "node-sass": "^4.8.3",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^12.0.0",
    "electron-rebuild": "^1.7.2",
    {{else}}
    "electron-builder": "^20.13.3",
    {{/if_eq}}
    {{#eslint}}
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-vue": "^4.0.0",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    {{/if_eq}}
    {{/eslint}}
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    {{#if unit}}
    {{#if_eq unitConfig 'mocha'}}
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "webpack-merge": "^4.1.0",
    {{else}}
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "vue-jest": "^1.0.2",
    "selenium-server": "^3.0.1",
    {{/if_eq}}
    {{/if}}
    {{#if e2e}}
    {{#if_eq unitConfig 'cypress'}}
    "require-dir": "^0.3.0",
    "spectron": "^3.7.1",
    {{else}}
    "nightwatch": "^0.9.12",
    {{/if_eq}}
    {{/if}}
    {{#testing unit e2e}}
    "babel-plugin-istanbul": "^4.1.1",
    "chai": "^4.0.0",
    "mocha": "^3.0.2",
    {{/testing}}
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-notifier": "^5.1.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.18.2"
    "pug": "^2.0.3",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-merge": "^4.1.0"
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
  }
}
